<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/transparent"
    tools:context=".out_room_modal"
    >

    <LinearLayout
        android:layout_width="138.67dp"
        android:layout_height="60.00dp"
        android:layout_gravity="right"
        android:background="@drawable/out_room"
        android:gravity="right"
        android:orientation="vertical"
        >

        <Button
            android:id="@+id/end_button"
            android:layout_width="138.67dp"
            android:layout_height="30.00dp"
            android:background="@android:color/transparent"
            android:text="회의 종료"
            android:textColor="#D40000"
            android:textSize="10.00sp" />

        <View
            android:layout_width="138.67dp"
            android:layout_height="0.33dp"
            android:background="#4D000000"
            />

        <Button
            android:id="@+id/out_button"
            android:layout_width="138.67dp"
            android:layout_height="30.00dp"
            android:background="@android:color/transparent"
            android:text="회의 나가기"
            android:textColor="#000000"
            android:textSize="10.00sp" />

    </LinearLayout>

</FrameLayout>